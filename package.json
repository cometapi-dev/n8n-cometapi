{
  "name": "n8n-nodes-cometapi",
  "version": "0.1.0",
  "description": "n8n nodes for FriendGrid CometApi",
  "keywords": ["n8n-community-node-package"],
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons"
  },
  "files": ["dist"],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": ["dist/nodes/CometApi/CometApi.node.js"],
    "credentials": ["dist/credentials/CometApiApi.credentials.js"]
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "n8n-workflow": "1.0.0",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "snowflake-sdk": "npm:empty-package@1.0.0",
    "@azure/storage-blob": "npm:empty-package@1.0.0"
  }
}
